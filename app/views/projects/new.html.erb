
<%= simple_form_for @project do |f| %>
<%= hidden_field_tag :subject_id, params[:subject_id] %>

	
	<%= f.input :title %>
	<%= f.input :image %>
	<%= f.input :body %>
	<%= f.input :url %>
	<%= f.collection_select :subject_id, Subject.order(:title),:id,:title %>

	<%= link_to "New Subject", new_subject_path %>

	<p><%= f.button :submit %></p>

<% end %>